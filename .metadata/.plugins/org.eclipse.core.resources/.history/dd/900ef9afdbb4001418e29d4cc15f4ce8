package core.JUnitParameterization40;

public class PrimeNumbers {

	public static void main(String[] args) {
		   int n, status = 1, num = 3;
		   
		   
	      for ( int count = 1 ; count <=111 ;  )
	      {
	         for ( int j = 2 ; j <= Math.sqrt(num) ; j++ )
	         {
	            if ( num%j == 0 )
	            {
	               status = 0;
	               break;
	            }
	         }
	         if ( status != 0 )
	         {
	            System.out.println(num);
	            count++;
	         }
	         status = 1;
	         num++;
	      }   
	}

}
